      if (typeof window.onload != 'function') {
        window.onload = newHandler;
      }

      else {
        var oldHandler = window.onload;
        window.onload = function () {
          if (oldHandler) {
            oldHandler();
          }
          newHandler();
        };
      }
    };

    addOnLoadHandler(
      function () {
